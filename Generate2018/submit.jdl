Universe = vanilla
Executable = wrapper.sh
arguments = $(proc) 2000
request_cpus = 8 
should_transfer_files = YES
Error = log/$(proc)/$(proc).err_$(Step)
Output = log/$(proc)/$(proc).out_$(Step)
Log = log/$(proc)/$(proc).log
transfer_input_files = /local-scratch/<username>/gridpacks/$(proc)_slc7_amd64_gcc700_CMSSW_10_6_0_tarball.tar.xz, /local-scratch/<username>/CMSSWGeneration/Generate2018/input/SMP-RunIIFall18wmLHEGS-00062_1_cfg.py,  /local-scratch/<username>/CMSSWGeneration/Generate2018/input/SMP-RunIIAutumn18DRPremix-00050_1_cfg.py, /local-scratch/<username>/CMSSWGeneration/Generate2018/input/SMP-RunIIAutumn18DRPremix-00050_2_cfg.py, /local-scratch/<username>/CMSSWGeneration/Generate2018/input/SMP-RunIIAutumn18MiniAOD-00050_1_cfg.py, /local-scratch/<username>/CMSSWGeneration/Generate2018/input/SMP-RunIIAutumn18NanoAODv7-00058_1_cfg.py, /local-scratch/<username>/CMSSWGeneration/Generate2018/input/CMSSW_10_2_6.tgz, /local-scratch/<username>/CMSSWGeneration/Generate2018/input/CMSSW_10_6_20.tgz
transfer_output_remaps = "SMP-RunIIAutumn18NanoAODv7-00058.root = /scratch/<username>/CMSSWGeneration/Generate2018/output/$(proc)/$(proc)_$(Cluster)_$(Step).root"
when_to_transfer_output = ON_EXIT
Queue 300 proc in (WZin_cHbox_LI)
