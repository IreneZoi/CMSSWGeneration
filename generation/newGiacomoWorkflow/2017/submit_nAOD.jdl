Universe = vanilla
Executable = wrapper_nAOD.sh

arguments = $(Step)
use_x509userproxy = true
+JobFlavour = "nextweek"
+SingularityImage = "/cvmfs/singularity.opensciencegrid.org/cmssw/cms:rhel7"
request_memory = 2100

should_transfer_files = YES
Error = log_nAOD/$(Step).err
